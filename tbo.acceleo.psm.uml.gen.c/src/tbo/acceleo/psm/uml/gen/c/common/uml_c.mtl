[**
 * @brief Implements the steps of the transition after the source state exit is 
 * completed.
 * @param aTransition The transition that should be implemented.
 * @details
 * @param indentation The indentation means the sum of all previous code 
 * indentations.
 * The following variables are initialized:
 * assign_choice_result - necessary, because ordinary States do not return entry
 *                 status, but Choice pseudostates return entry status.
 * sync_vertices - When the transition is executed after the effect behavior
 *                 is completed, a number of states may be entered before
 *                 the target state is reached. Some of them have entry 
 *                 behaviors that are executed synchronously, some need
 *                 some time to be completed and must be called asynchronously.
 *                 This list contains all states that must be entered directly
 *                 by the transition, and that can be entered by a synchronous 
 *                 call. Only the last vertex on this list is allowed to have 
 *                 asynchronous call, since it will trigger the rest of the
 *                 state entry chain.
 */]
[template public uml_c_implement_transition_transit(aTransition:Transition,
                                                   indentation : Integer)
   ?(not aTransition.oclIsUndefined())
   post(trim())
   {assign_choice_result : String = 
        if aTransition.target.request_vertex_is_choice() then 
            utils_dispatch_result_name() + ' = ' 
        else '' endif;
    assign_result : String = utils_dispatch_result_name() + ' = ';
    temp_name : String = request_temp_dispatch_result_name();
    assign_temp_status : String = temp_name + ' = ';
    sync_vertices : OrderedSet(Vertex) = 
                 request_transition_target_sync_vertices(aTransition);}]
[if not aTransition.effect.oclIsUndefined()]
[uml_c_implement_transition_effect(aTransition, indentation)/]
[/if]
[if not aTransition.effect.request_behavior_needs_async_call()]
[comment first enter all states that are directly entered by the transition/]
[for (aVertex:Vertex | sync_vertices)]
[if aVertex = aTransition.target]
[comment This is executed only if all state entries up to the target vertex
 are executed synchronously/]
[debug_marker('uml_c_implement_transition_transit 1')
/][assign_choice_result/][uml_c_format_param_list(aVertex.uml_c_get_entry_name(),
                                         Sequence{ utils_get_self_name()}, 
                    indentation + assign_choice_result.size() + 1)/];
[else]
[comment All states that appear on the path to the target vertex, must be 
 entered/]
[uml_c_format_param_list(aVertex.uml_c_get_entry_name(),
         Sequence{ utils_get_self_name()}, indentation+1)/];
[/if]
[/for]
[if aTransition.request_is_transition_local_to_parent()]
[comment In case of a local transition to parent the sync_vertices list
  is empty and it is ok/]
[utils_dispatch_result_name()/] = [request_transition_changed_state()/];
[elseif sync_vertices->isEmpty()]
[comment This is not the case when the target vertex is a Choice pseudostate./]
[utils_dispatch_result_name()/] = [request_transition_transit_state()/];
[elseif sync_vertices->last() <> aTransition.target
        or sync_vertices->last().request_vertex_needs_async_entry()]
[comment There are still some asynchronous calls that must be completed./]
[utils_dispatch_result_name()/] = [request_transition_transit_state()/];
[elseif not aTransition.target.request_vertex_is_choice()]
[comment This line is needed only if the target is not a Choice pseudostate,
         since for a Choice pseudostate the result is assigned above in the 
         for loop./]
[utils_dispatch_result_name()/] = [request_transition_changed_state()/];
[/if]
[comment then initialize all other concurrent regions that are
 activated as a side effect/]
[for (aRegion:Region | request_transition_target_sync_regions_init(aTransition))
     before(uml_c_get_event_status_type() + ' ' + assign_temp_status 
            + request_transition_ignored_state() + ';' + utils_newline())]
[assign_temp_status/][uml_c_format_param_list(
         aRegion.uml_c_get_region_entry_name(),
         Sequence{ utils_get_self_name()}, 
         indentation + assign_temp_status.size() + 1)/];
[assign_result/][uml_c_format_param_list(
                    uml_c_get_event_resolve_status_name(),
                    Sequence{ utils_dispatch_result_name(), temp_name}, 
                    indentation+assign_result.size()+1)/];
[/for]
[else]
[comment When the transition effect behavior needs an asynchronous call, then 
 the transition must remain in the Transit state./]
[utils_dispatch_result_name()/] = [request_transition_transit_state()/];
[/if]
[/template]

