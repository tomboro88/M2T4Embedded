[query public uml_c_getjunction_entry_id(aSeq : Sequence(Transition)):String
 = if aSeq->notEmpty() and aSeq->last().target.request_is_static_junction() then 
      '_' + aSeq->first().containingStateMachine().eAllContents(Transition)
          ->indexOf(aSeq->first()).toString()
             .concat(aSeq->drop(1)->uml_c_getjunction_entry_id())else '' endif/]

[query public uml_c_get_junction_entry_name(aSeq : Sequence(Transition), 
                                            anEvent : Event) 
 : String = if aSeq->notEmpty() 
            and aSeq->last().target.request_is_static_junction()then 
               (aSeq->last().containingStateMachine().name + '_enter_' 
               + aSeq->last().target.name 
               + if anEvent.oclIsUndefined() then '' 
                    else '_' + anEvent.name endif
               + aSeq->uml_c_getjunction_entry_id()).toLower() 
            else ' #error Empty transition sequence' endif/]

