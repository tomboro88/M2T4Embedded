                                                      indentation : Integer)
    {aState:State = aTransition.request_transition_exit_state();
     exit_result : String = 'bool b_is_completed = ';
     spaces : String = utils_get_indent_string();}]
[if aTransition.kind = TransitionKind::internal and
    aTransition.source.oclIsKindOf(State)]
[debug_marker('uml_c_implement_transition_execution 1')
/][if not aTransition.effect.oclIsUndefined()]
[uml_c_implement_transition_effect(aTransition, indentation)/]
[/if]
[utils_dispatch_result_name()
                         /] = [aTransition.request_transition_dispatch_result() 
/];[else]
[if aTransition.request_transition_needs_exit() and 
    not (aTransition.source.request_vertex_is_choice() 
         and (aTransition.source.container 
                         = aTransition.request_transition_effect_region())
         and aTransition.request_transition_is_local())]
[if aTransition.request_transition_is_local()]
[comment local transitions require the exit of the transition topmost region/]
[debug_marker('uml_c_implement_transition_execution 2')
/][exit_result/][ uml_c_format_param_list(
         aTransition.request_transition_effect_region()
                                                  .uml_c_get_region_exit_name(),
